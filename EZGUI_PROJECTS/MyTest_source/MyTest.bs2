SUB EZ_DesignWindow(FormName$)     ' (PROTECTED)
     SELECT CASE FormName$
          CASE "NEWFORM"
               EZ_NEWFORM_Design
          CASE "SETUPFORM"
               EZ_SETUPFORM_Design
          CASE ELSE
               OtherForm_Design FormName$
     END SELECT
END SUB

