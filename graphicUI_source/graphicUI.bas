SUB KEDIT_Events(CID&, CMsg&, CVal&, Cancel&)
     SELECT CASE CID&
          CASE %EZ_Window
               SELECT CASE CMsg&
                    CASE %EZ_Loading
                        EZ_SetLoadStr "T", App_Prompt
                    CASE %EZ_Loaded
                    CASE %EZ_Started
                    CASE %EZ_Close
                        EZ_PostEvent "{ME}", 0, 1
                    CASE %EZ_Post
                        EZ_ResetFocus
                    CASE ELSE
               END SELECT
          CASE ELSE
     END SELECT
END SUB

SUB KEDIT_TEXT1_Events( MyID&, CMsg&, CVal&, Cancel&)
     SELECT CASE CMsg&
          CASE %EZ_Loading
              IF App_TMask<>"" THEN
                 IF LEN(App_Edit)=LEN(App_TMask) THEN
'                     EZ_SetLoadStr "P", "EST"+"{"+App_TMask+"}"
                 END IF
              END IF
              EZ_SetLoadStr "T", App_Edit
          CASE %EZ_Change
          CASE ELSE
     END SELECT
END SUB

SUB KEDIT_APPLYBUTTON_Events( MyID&, CMsg&, CVal&, Cancel&)
     SELECT CASE CMsg&
          CASE %EZ_Click
              App_Edit = EZ_GetText("KEDIT", %KEDIT_TEXT1)
              App_ExitCode = 13
              EZ_UnloadForm "{ME}"
          CASE ELSE
     END SELECT
END SUB

SUB KEDIT_CANCELBUTTON_Events( MyID&, CMsg&, CVal&, Cancel&)
     SELECT CASE CMsg&
          CASE %EZ_Click
             App_ExitCode = 27
              EZ_UnloadForm "{ME}"
          CASE ELSE
     END SELECT
END SUB



