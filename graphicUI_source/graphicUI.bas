MACRO EZ_PBMAIN = EZ_PBMAINFUNC AS LONG
GLOBAL App_RunPlotDone&

SUB MAIN_Events(CID&, CMsg&, CVal&, Cancel&)
     SELECT CASE CID&
          CASE %EZ_Window
               SELECT CASE CMsg&
                    CASE %EZ_NoAutoSize
                        Cancel&=1
                    CASE %EZ_Loading
                    CASE %EZ_Loaded
                    CASE %EZ_Started
                        LOCAL X&
                        EZ_MaximizeForm "MAIN"
                        App_RunPlotDone& = 0
                        #IF %IntegrateCCApp
                            X& = EZ_PBMAINFUNC()
                            EZ_UnloadForm "FLOAT1"
                        #ELSE
                            EZ_KEDIT_Display "Main"
                            EZ_FLOAT1_Display "Main"
                        #ENDIF
                        EZ_SetText "MAIN", %MAIN_LABEL1, "Display Ended!
                        App_RunPlotDone& = 1
                    CASE %EZ_Close
                        IF App_RunPlotDone& = 0 THEN
                            Cancel& =  -1
                        END IF
                    CASE %EZ_Post
                        IF CVal&=%App_ShowFloat THEN
                            EZ_FLOAT1_Display "MAIN"
                        END IF
                    CASE ELSE
               END SELECT
          CASE ELSE
     END SELECT
END SUB



