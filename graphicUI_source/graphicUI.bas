SUB EZ_KEDIT_ParseEvents(CID&, CMsg&, CVal&, Cancel&)     ' (PROTECTED)
     SELECT CASE CID&
          CASE %EZ_Window
               KEDIT_Events CID&, CMsg&, CVal&, Cancel&
          CASE  %KEDIT_TEXT1
               KEDIT_TEXT1_Events CID&, CMsg&, CVal&, Cancel&
          CASE  %KEDIT_APPLYBUTTON
               KEDIT_APPLYBUTTON_Events CID&, CMsg&, CVal&, Cancel&
               IF CMsg&=%EZ_OwnerDraw THEN
                    EZ_Draw3DButtonRR "KEDIT", %KEDIT_APPLYBUTTON, CVal&, 10, 0,  6
               END IF
          CASE  %KEDIT_CANCELBUTTON
               KEDIT_CANCELBUTTON_Events CID&, CMsg&, CVal&, Cancel&
               IF CMsg&=%EZ_OwnerDraw THEN
                    EZ_Draw3DButtonRR "KEDIT", %KEDIT_CANCELBUTTON, CVal&, 20, 0,  6
               END IF
          CASE ELSE
               KEDIT_Events CID&, CMsg&, CVal&, Cancel&
     END SELECT
END SUB

