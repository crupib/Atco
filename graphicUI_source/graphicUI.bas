SUB EZ_ResetFocus()
    DIM P(1 TO 2) AS INTEGER
    P(1)=0
    P(2)=0
    SendMessage App_hGraphicWindow, %WM_NCLBUTTONDOWN, %HTCAPTION, BYVAL VARPTR(P(1))
    SendMessage App_hGraphicWindow, %WM_NCLBUTTONUP, %HTCAPTION, BYVAL VARPTR(P(1))
END SUB

GLOBAL App_FakeKey$

SUB EZ_AddKey(BYVAL T$)
    App_FakeKey$=App_FakeKey$+T$
END SUB

SUB EZ_CheckEvents()
    EZ_DoEvents 5
END SUB

FUNCTION EZ_GraphicInstat() AS LONG
    LOCAL RV&
    RV&=0
    GRAPHIC INSTAT TO RV&
    IF RV&=0 THEN
        IF LEN(App_FakeKey$)<>0 THEN RV&=1
    END IF
    FUNCTION=RV&
END FUNCTION

FUNCTION EZ_GraphicInkey$()
    LOCAL RV$
    RV$=""
    GRAPHIC INKEY$ TO RV$
    IF RV$="" THEN  ' test for EZGUI controls as fake keys
        IF LEN(App_FakeKey$)<>0 THEN
            IF ASC(MID$(App_FakeKey$,1,1))=0 THEN
                ' two codes
                RV$=MID$(App_FakeKey$,1,2)
                App_FakeKey$=MID$(App_FakeKey$,3)
            ELSE
                ' one code
                RV$=MID$(App_FakeKey$,1,1)
                App_FakeKey$=MID$(App_FakeKey$,2)
            END IF
        END IF
    END IF
    FUNCTION=RV$
END FUNCTION
'<<END>>

